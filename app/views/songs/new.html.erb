<h1> Create A Song </h1>  
    
    <%= form_for(@song) do |s| %>

        <label> Song Name: </label>
            <%= s.text_field :name %>

        <label> Artist Id: </label>
            <%= s.text_field :artist_id %>

        <label> Genre Id: </label>
            <%= s.text_field :genre_id %>

        <%= s.submit %>

    <% end %>


    